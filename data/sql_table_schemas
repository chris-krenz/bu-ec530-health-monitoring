CREATE TABLE `users` (
  `pid` integer PRIMARY KEY,
  `first_name` varchar(255),
  `last_name` varchar(255),
  `dob` date,
  `ssn` varchar(255),
  `address_1` varchar(255),
  `unit` varchar(255),
  `city` varchar(255),
  `state` varchar(255),
  `zip` varchar(255),
  `country` varchar(255),
  `created_at` timestamp
);


